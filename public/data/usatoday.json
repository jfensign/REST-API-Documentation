{
   "endpoints":[
      {
         "name":"Census",
         "methods":[
            {
               "MethodName":"Locations",
               "Synopsis":"Returns all available ethnicity, housing, population and race information for specified area. ",
               "HTTPMethod":"GET",
               "URI":"/census/locations",
               "RequiresOAuth":"N",
               "parameters":[
                  {
                     "Name":"keypat",
                     "Required":"N",
                     "Default":"",
                     "Type":"string",
                     "Description":"State abbreviation default, or arbitrary placename if keyname=placename"
                  },
                  {
                     "Name":"keyname",
                     "Required":"N",
                     "Default":"",
                     "Type":"string",
                     "Description":"[placename, FIPS, GNIS]"
                  },
                  {
                     "Name":"sumlevid",
                     "Required":"N",
                     "Default":"2",
                     "Type":"enumerated",
                     "EnumeratedList": [
                        "1",
                        "2",
                        "3",
                        "4"
                     ],
                     "EnumeratedDescription": {
                        "1": "National level",
                        "2": "State level",
                        "3": "County level",
                        "4": "City, town level"
                     },
                     "Description":"Use sumlevid to define what level of census data you would like."
                  }
               ]
            },
